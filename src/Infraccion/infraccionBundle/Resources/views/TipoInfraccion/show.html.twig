{% extends '::layout.html.twig' %}
{% block id 'mod_TipoInfraccion' %}
{% block title %}
{{ parent() }} - Ver Tipo de Infraccion
{% endblock %}

{% block page %}

<section id="view_TipoInfraccion">
    {% embed 'box.html.twig' with {'box_titulo': 'Ver Tipo de Infraccion' } %}
    {% block box_contenido %}
    <div class="row-fluid">
        <div class="span12" style="margin-top: 0;">
            {{ wo_render_breadcrumbs() }}

            {% for type, flashMessages in app.session.flashbag.all() %}
             {% for flashMessage in flashMessages %} 
             <div class='alert alert-{{ type }}'> 
             {{ flashMessage|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
             </div> 
             {% endfor %} 
            {% endfor %} 
        </div>
    </div>


    <div class="row-fluid" style="margin-top: 0;">
        <div class="span12" style="zoom: 1; overflow: auto; margin-top: -8px;">
            <form class="form-horizontal">
                <fieldset>                        <div class="control-group">
                            <label class="control-label">Id</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.id }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Codigo</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.codigo }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Nombre</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.nombre }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Observacion</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.observacion }}</span>                            </div>
                        </div>                </fieldset>
            </form>
        </div>
    </div>

        <div class="row-fluid">
    <div class="offset1 span2">
        <a class="btn" href="{{ path('tipoinfraccion') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="span2">
        <a class="btn btn-primary" href="{{ path('tipoinfraccion_edit', { 'id': entity.id }) }}">
            {{ 'views.recordactions.edit'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left">
        <form action="{{ path('tipoinfraccion_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button class="btn btn-danger" type="submit" onclick="return confirm('{{ 'views.recordactions.confirm'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}');">{{ 'views.recordactions.delete'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}</button>
        </form>
    </div>
</div>    {% endblock box_contenido %}
    {% endembed %}
</section>
{% endblock %}