namespace Soporte\UsuarioBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Symfony\Component\Validator\Constraints as Assert;


/**
 * Soporte\UsuarioBundle\Entity\RoleUsuario
 *
 * @ORM\Entity(repositoryClass="Soporte\UsuarioBundle\Entity\RoleUsuarioRepository")
  */
class RoleUsuario
{

    /**
     * @ORM\Id
     * @ORM\ManyToOne(targetEntity="Soporte\UsuarioBundle\Entity\Usuario")
     */
    protected $usuario;

    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     */
    protected $role_id;

    /** @ORM\Column(type="datetime") */
    protected $fecha_asignada;



}