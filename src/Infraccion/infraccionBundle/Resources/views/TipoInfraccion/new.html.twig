{% extends '::layout.html.twig' %}
{% block id 'mod_TipoInfraccion' %}
{% block title %}
{{ parent() }} - TipoInfraccion {{ 'views.new.creation'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
{% endblock %}

{% block page %}

<section id="new_TipoInfraccion">
    {% embed 'box.html.twig' with {'box_titulo': 'Nuevo TipoInfraccion' } %}
    {% block box_contenido %}
    <div class="row-fluid">
        <div class="span12" style="margin-top: 0;">
            {{ wo_render_breadcrumbs() }}

            {% for type, flashMessages in app.session.flashbag.all() %}
             {% for flashMessage in flashMessages %} 
             <div class='alert alert-{{ type }}'> 
             {{ flashMessage|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
             </div> 
             {% endfor %} 
            {% endfor %} 
        </div>
    </div>


    <div class="row-fluid" style="margin-top: 0;">
        <div class="span12" style="zoom: 1; overflow: auto; margin-top: -8px;">
            <form class="form-horizontal" action="{{ path('tipoinfraccion_create') }}"
                  method="post" {{ form_enctype(form) }}>
                {{ form_widget(form) }}
                <p>
                    <button type="submit"
                            class="btn btn-success">{{ 'views.new.create'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}</button>
                </p>
            </form>
        </div>
    </div>
        <div class="row-fluid">
    <div class="offset1 span2">
        <a class="btn" href="{{ path('tipoinfraccion') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
</div>
    {% endblock box_contenido %}
    {% endembed %}

</section>

{% endblock %}