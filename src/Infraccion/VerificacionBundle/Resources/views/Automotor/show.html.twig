{% extends '::layout.html.twig' %}
{% block id 'mod_Automotor' %}
    {% block title %}
        {{ parent() }} - Automotor {{ 'views.show.show'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
    {% endblock %}

    {% block page %}

        <section id="view_Automotor">
            {% embed 'box.html.twig' with {'box_titulo': 'Automotor' } %}
                {% block box_contenido %}
                    <div class="row-fluid">
                        <div class="span12" style="margin-top: 0;">
                            {{ wo_render_breadcrumbs() }}

                            {% for type, flashMessages in app.session.flashbag.all() %}
                                {% for flashMessage in flashMessages %}
                                    <div class='alert alert-{{ type }}'>
                                        {{ flashMessage|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
                                    </div>
                                {% endfor %}
                            {% endfor %}
                        </div>
                    </div>
                    <div class="row-fluid" style="margin-top: 0;">
                        <div class="span12" style="zoom: 1; overflow: auto; margin-top: -8px;">

                            <form class="form-horizontal">
                                <fieldset>
                                    <div class="control-group">
                                        <label class="control-label">Id</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.id }}</span></div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Dominio</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.dominio }}</span>
                                        </div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Marca</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.marca }}</span>
                                        </div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Modelo</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.modelo }}</span>
                                        </div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Dni</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.dni }}</span></div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Cuitcuil</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.cuitCuil }}</span>
                                        </div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Nombre</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.nombre }}</span>
                                        </div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Domicilio</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.domicilio }}</span>
                                        </div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Codigopostal</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.codigoPostal }}</span>
                                        </div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Provincia</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.provincia }}</span>
                                        </div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Localidad</label>

                                        <div class="controls"><span
                                                    class="input-xlarge uneditable-input">{{ entity.localidad }}</span>
                                        </div>
                                    </div>
                                </fieldset>
                            </form>
                        </div>
                    </div>

                    <div class="row-fluid">
                        <div class="offset1 span2">
                            <a class="btn" href="{{ path('automotor') }}">
                                {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
                            </a>
                        </div>
                    </div>
                {% endblock box_contenido %}
            {% endembed %}

        </section>

    {% endblock %}