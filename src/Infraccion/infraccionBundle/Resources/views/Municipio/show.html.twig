{% extends '::layout.html.twig' %}
{% block id 'mod_Municipio' %}
{% block title %}
{{ parent() }} - Municipio {{ 'views.show.show'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
{% endblock %}

{% block page %}

<section id="view_Municipio">
    {% embed 'box.html.twig' with {'box_titulo': 'Ver Municipio' } %}
    {% block box_contenido %}
    <div class="row-fluid">
        <div class="span12" style="margin-top: 0;">
            {{ wo_render_breadcrumbs() }}

            {% for type, flashMessages in app.session.flashbag.all() %}
             {% for flashMessage in flashMessages %} 
             <div class='alert alert-{{ type }}'> 
             {{ flashMessage|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
             </div> 
             {% endfor %} 
            {% endfor %} 
        </div>
    </div>


    <div class="row-fluid" style="margin-top: 0;">
        <div class="span12" style="zoom: 1; overflow: auto; margin-top: -8px;">
            <form class="form-horizontal">
                <fieldset>                        <div class="control-group">
                            <label class="control-label">Id</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.id }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Codigo</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.codigo }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Nombre</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.nombre }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Email</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.email }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Telefono</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.telefono }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Direccion</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.direccion }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Localidad</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.localidad }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Codigopostal</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.codigoPostal }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Cont1nombre</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.cont1Nombre }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Cont1cargo</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.cont1Cargo }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Cont1celular</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.cont1Celular }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Cont1telfijo</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.cont1Telfijo }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Cont1email</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.cont1Email }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Cont2nombre</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.cont2Nombre }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Cont2cargo</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.cont2Cargo }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Cont2celular</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.cont2Celular }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Cont2telfijo</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.cont2Telfijo }}</span>                            </div>
                        </div>                        <div class="control-group">
                            <label class="control-label">Cont2email</label>

                            <div class="controls">                                    <span class="input-xlarge uneditable-input">{{ entity.cont2Email }}</span>                            </div>
                        </div>                </fieldset>
            </form>
        </div>
    </div>

        <div class="row-fluid">
    <div class="offset1 span2">
        <a class="btn" href="{{ path('municipio') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="span2">
        <a class="btn btn-primary" href="{{ path('municipio_edit', { 'id': entity.id }) }}">
            {{ 'views.recordactions.edit'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left">
        <form action="{{ path('municipio_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button class="btn btn-danger" type="submit" onclick="return confirm('{{ 'views.recordactions.confirm'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}');">{{ 'views.recordactions.delete'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}</button>
        </form>
    </div>
</div>    {% endblock box_contenido %}
    {% endembed %}
</section>
{% endblock %}